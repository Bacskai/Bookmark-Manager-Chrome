{
  "manifest_version": 3,
  "name": "Bookmark Manager Extension",
  "version": "1.0",
  "description": "A Chrome extension for managing bookmarks.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage", "activeTab" , "cookies"] 
}
